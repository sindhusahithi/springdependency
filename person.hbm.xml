<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.onetoone.Person" table="person">

<id name="personid" column="personid"  />
<property name="personname" column="personname" length="20"/>
<property name="personage" column="personage" length="20"/>

</class>
</hibernate-mapping>